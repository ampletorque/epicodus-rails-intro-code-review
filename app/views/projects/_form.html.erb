<%= render('layouts/errors', :object => @project) %>

<%= form_for [@user, @project] do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :short_description %>
  <%= f.text_field :short_description %>

  <%= f.label :long_description %>
  <%= f.text_field :long_description %>

  <%= f.label :category %>
  <%= f.text_field :category %>

  <%= f.label :coworkers %>
  <%= f.text_field :coworkers %>

  <%= f.label :code_url %>
  <%= f.text_field :code_url %>

  <%= f.label :hosted_url %>
  <%= f.text_field :hosted_url %>

  <%= f.label :project_date %>
  <%= f.text_field :project_date %>

  <p><%= f.submit %></p>
<% end %>
