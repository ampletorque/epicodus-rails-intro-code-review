<%= render('layouts/errors', :object => @skill) %>

<%= form_for [@user, @skill] do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :description %>
  <%= f.text_field :description %>

  <%= f.label :category %>
  <%= f.text_field :category %>

  <p><%= f.submit %></p>
<% end %>
