<%= render "layouts/errors", :object => @user %>

<%= form_for @user do |f| %>

<%= f.label :name %>
<%= f.text_field :name %>

<%= f.label :short_description %>
<%= f.text_field :short_description %>

<%= f.label :long_description %>
<%= f.text_field :long_description %>

<%= f.label :title %>
<%= f.text_field :title %>

<%= f.label :app_role %>
<%= f.text_field :app_role %>

<%= f.label :phone %>
<%= f.text_field :phone %>

<%= f.label :email %>
<%= f.text_field :email %>

<%= f.label :code_url %>
<%= f.text_field :code_url %>

<%= f.label :hosted_url %>
<%= f.text_field :hosted_url %>

<%= f.submit  %>

<% end %>
